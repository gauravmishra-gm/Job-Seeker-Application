# Copy this file to config.env and fill in real values. Do NOT commit secrets.

PORT=4000

# Cloudinary (example placeholders)
CLOUDINARY_CLIENT_NAME=your_cloud_name
CLOUDINARY_CLIENT_API=your_api_key
CLOUDINARY_CLIENT_SECRET=your_api_secret

# Frontend URL
FRONTEND_URL=http://localhost:5173

# MongoDB connection URI
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>/<db>?retryWrites=true&w=majority

# JWT
JWT_SECRET_KEY=change_this_secret
JWT_EXPIRE=7d
COOKIE_EXPIRE=5


